  <% @posts.each do |post| %>
    <tr>
      <td>
        <%= post.id %>
      </td>
      <td>
        <%= post.date %>
      </td>
      <td>
        <%= post.body %>
      </td>
      <td>
        <%= post.user.full_name %>
      </td>
       <td>
        <%= link_to 'Edit', edit_post_path(post), id: "edit_#{post.id}" %>
      </td>
    </tr>
  <% end %>